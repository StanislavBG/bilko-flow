{
  "name": "bilko-flow",
  "version": "0.3.0",
  "description": "TypeScript library for VibeCoders and AI agents to design, validate, and execute deterministic workflows from natural-language goals",
  "author": "bilko-flow contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/StanislavBG/bilko-flow.git"
  },
  "homepage": "https://github.com/StanislavBG/bilko-flow#readme",
  "bugs": {
    "url": "https://github.com/StanislavBG/bilko-flow/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./react/components": {
      "types": "./dist/react/components.d.ts",
      "default": "./dist/react/components.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "build": "tsc && cp src/styles.css dist/styles.css",
    "build:css": "cp src/styles.css dist/styles.css",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "workflow",
    "deterministic",
    "orchestration",
    "dsl",
    "ai-agent",
    "planner",
    "vibecoding"
  ],
  "license": "MIT",
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "lucide-react": ">=0.300.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "lucide-react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",

    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
